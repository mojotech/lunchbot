<section id="torch-toolbar">
  <div class="torch-container">
    <%= link "Edit", to: Routes.admin_item_option_headings_path(@conn, :edit, @item_option_headings), class: "torch-button" %>
    <%= link "Back", to: Routes.admin_item_option_headings_path(@conn, :index), class: "torch-button" %>
  </div>
</section>

<section id="torch-header-and-content">
  <div class="torch-container">
    <header class="header">
      <h3>Item option headings Details</h3>
    </header>
    <section class="torch-show-details">
      
        <div class="torch-show-attribute">
          <div class="torch-show-label">Item:</div>
          <div class="torch-show-data"><%= @item_option_headings.item_id %></div>
        </div>
      
        <div class="torch-show-attribute">
          <div class="torch-show-label">Option heading:</div>
          <div class="torch-show-data"><%= @item_option_headings.option_heading_id %></div>
        </div>
      
    </section>
  </div>
</section>
